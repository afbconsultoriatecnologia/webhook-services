# Application
NODE_ENV=production
PORT=4000
LOG_LEVEL=info

# Database
DATABASE_URL="postgresql://user:password@localhost:5432/database?schema=schema"

# STT Configuration
STT_WEBHOOK_URL=https://api.example.com/webhook
STT_WEBHOOK_TOKEN=Bearer your-token-here
STT_CRON_TOKEN=your-cron-token-here
STT_MODO_TESTE=true

# Security
ALLOWED_IPS=127.0.0.1,::1
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=10

# Timeouts
LOCK_TIMEOUT_MINUTES=5
HTTP_TIMEOUT_MS=30000
QUERY_TIMEOUT_MS=30000

# Frontend URL (for PDF links)
FRONTEND_URL=https://webhook-telemedicina.informarsaude.com.br